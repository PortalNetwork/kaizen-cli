
{
    "presets": [
      "stage-2",
      [
        "env",
        {
          "targets": {
            "browsers": [
              "> 1%",
              "last 5 versions",
              "Firefox >= 45",
              "iOS >=8",
              "Safari >=8",
              "ie >= 10"
            ]
          }
        }
      ]
    ],
    "plugins": [
      "babel-plugin-transform-class-properties",
      "transform-async-to-generator",
      "transform-export-extensions",
      [
        "transform-runtime",
        {
          "polyfill": false,
          "regenerator": true
        }
      ]
    ]
  }